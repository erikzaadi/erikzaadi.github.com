---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset
      xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
            http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">
    {% for post in site.posts %}
    <url>
        <loc>http://projects.erikzaadi.com{{ post.url }}</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>{% endfor %}{% for project in site.projects %}
    <url>
        <loc>http://projects.erikzaadi.com{{ project.link }}</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>{% endfor %}
    <url>
        <loc>http://projects.erikzaadi.com/posts.html</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>
    <url>
        <loc>http://projects.erikzaadi.com/projects.html</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>1.00</priority>
    </url>
</urlset>