---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Erik Zaadi @ GitHub</title>
 <link href="http://erikzaadi.github.com/atom.xml" rel="self"/>
 <link href="http://erikzaadi.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>erikzaadi.github.com</id>
 <author>
   <name>Erik Zaadi</name>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://erikzaadi.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://erikzaadi.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
 